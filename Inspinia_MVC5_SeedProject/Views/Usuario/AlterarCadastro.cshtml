@model

<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Preencha seu CEP para continuar o cadastro.</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <form role="form" class="form-inline">
                    <div class="form-group col-sm-offset-2">
                        <label for="exampleInputEmail2" class="sr-only">CEP:</label>
                        <input type="text" placeholder="Digite seu CEP" class="form-control">
                    </div>
                    <button class="btn btn-info col-sm-offset-4" type="submit">Consultar CEP</button>
                </form>
            </div>
        </div>
    </div> 
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Cadastro <small>(Preencha todos os campos antes de confirmar.)</small></h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <form method="get" class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Endereço:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1  control-label">Numero:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Bairro:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Complemento:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Cidade:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Estado:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">CPF:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">RG:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Data Nasc:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Sexo:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Telefone:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group ">
                        <label class="col-sm-2 col-sm-offset-1 control-label">Celular:</label>
                        <div class="col-sm-6"><input type="text" class="form-control"></div>
                    </div>
                    
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                         <div class="col-sm-6 col-sm-offset-2">
                            <div><label> <input type="checkbox" value=""> Desejo receber email com as novidades dos serviços SystemFarma.
</label></div>
                            <div><label> <input type="checkbox" value=""> Desejo receber SMS com as novidades dos serviços SystemFarma.
</label></div>
                        </div>
                    </div>
                    
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <div class="col-sm-4 col-sm-offset-2">
                            <button class="btn btn-white" type="submit">Cancelar</button>
                            <button class="btn btn-info" type="submit">Salvar Alterações</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
